<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="oval"  >
            <corners android:radius="5dip" />
            <stroke android:width="1dip" android:color="@android:color/holo_blue_light" />
            <gradient android:angle="-90" android:startColor="@android:color/holo_blue_bright" android:endColor="@android:color/holo_blue_bright"  />
        </shape>
    </item>

    <item android:state_selected="true" >
        <shape android:shape="oval"  >
            <corners android:radius="5dip" />
            <stroke android:width="1dip" android:color="@android:color/holo_blue_light" />
            <gradient android:angle="-90" android:startColor="@android:color/holo_blue_light" android:endColor="@android:color/holo_blue_dark"  />
        </shape>
    </item>

    <item android:state_focused="true">
        <shape android:shape="oval"  >
            <corners android:radius="5dip" />
            <stroke android:width="1dip" android:color="#eeeeee" />
            <solid android:color="#ed430f"/>
        </shape>
    </item>
    <item >
        <shape android:shape="oval"  >
            <corners android:radius="5dip" />
            <stroke android:width="1dip" android:color="@android:color/holo_blue_light" />
            <gradient android:angle="-90" android:startColor="@android:color/holo_blue_light" android:endColor="@android:color/holo_blue_dark" />
        </shape>
    </item>
</selector>